<!--* Any given genre's show page should have links to each of its artists and songs.-->
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>a Genre's artists and songs</title>
  </head>
  <body>
    <h2><%=@genre.name%></h2>
    <%@genre.songs.each do |song|%>
    <a href="/songs/<%=song.slug%>">
    <%song.name%> 
    </a> 
    <% end %>
    <% binding.pry %>
  </body>
</html>
