<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h2><%= @artist.name %></h2>
    <%@artist.songs.each do |song|%>
      <a href="/songs/<%= song.slug%>"
      >
      <%=song.name%>
      </a>
    <% end %>
    <%@artist.genres.each do |genre|%>
    <a href="/genres/<%=genre.slug%>"
    >
    <%=genre.name%>
    </a>
    <% end %>
    <%binding.pry%>
    </body>
  
